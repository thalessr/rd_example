<%= form_tag @leads do %>
  
      <%= label_tag :name %>
      <%= text_field_tag :name %>
    
      <%= label_tag :last_name %>
      <%= text_field_tag :last_name %>

     <%= label_tag :email %>
     <%= email_field_tag :email %> 

      <%= label_tag :company %>
      <%= text_field_tag :company %>

      <%= label_tag :phone %>
      <%= telephone_field_tag :phone %>

      <%= label_tag :job_tile %>
      <%= text_field_tag :job_tile %>

      <%= label_tag :website %>
      <%= text_field_tag :website %>

      <%= submit_tag "Create"%>
<% end %>